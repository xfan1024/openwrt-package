config client client_sample
    option localaddr    '127.0.0.1:1000'
    option remoteaddr   '8.8.8.8:1000'
    option key          'your_password'
    option crypt        'tea'
    option mode         'fast2'
    option conn         '4'
    option autoexpire   '120'
    option mtu          '1350'
    option sndwnd       '128'
    option rcvwnd       '768'
    option nocomp       '0'
    option sockbuf      '4194304'
    option keepalive    '15'
    option disabled     '1'
